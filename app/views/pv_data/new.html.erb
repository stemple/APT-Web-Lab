<h1>New PV_Data</h1>
<%= form_for @pv_datum do |f| %>

    <% if @pv_datum.errors.any? %>
        <div id="error_explanation">
          <h2>
            <%= pluralize(@pv_datum.errors.count, "error") %> prohibited
            this PV Datum from being saved:
          </h2>
